{
  "name": "publishing-client",
  "private": true,
  "type": "module",
  "workspaces": [
    "extensions/*",
    "extensions/vscode/webviews/*"
  ],
  "scripts": {
    "format": "prettier . --write",
    "check-format": "prettier . --check",
    "prepare": "husky"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "3.3.3"
  }
}
