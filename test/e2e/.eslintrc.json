{
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:cypress/recommended",
    "plugin:mocha/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "semi": "error",
    "mocha/no-mocha-arrows": "off",
    "mocha/no-exclusive-tests": "error"
  }
}
