{
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.cjs --max-warnings 0 .",
    "fix": "eslint --ext .js,.ts,.cjs . --fix",
    "init": "just ../../run init ./test/sample-content/fastapi-simple",
    "start": "just ../../run ui --listen 127.0.0.1:9000 ./test/sample-content/fastapi-simple",
    "test": "start-server-and-test --expect 200 start http-get://127.0.0.1:9000 run",
    "test-deploy": "start-server-and-test --expect 200 start http-get://127.0.0.1:9000 run-deploy",
    "test-deploy-open": "start-server-and-test --expect 200 start http-get://127.0.0.1:9000 run-deploy-open",
    "test-deploy-windows": "start-server-and-test --expect 200 start http-get://127.0.0.1:9000 run-deploy-windows",
    "open": "cypress open",
    "run": "cypress run --spec ./cypress/e2e/ui/*.cy.js --env token=token",
    "run-deploy": "cypress run --spec ./cypress/e2e/contract/*.cy.js --env token=token,CYPRESS_CONNECT_ADDRESS=$CYPRESS_CONNECT_ADDRESS,CONNECT_API_KEY=$CONNECT_API_KEY",
    "run-deploy-open": "cypress run --headed --no-exit --spec ./cypress/e2e/contract/*.cy.js --env token=token,CYPRESS_CONNECT_ADDRESS=$CYPRESS_CONNECT_ADDRESS,CONNECT_API_KEY=$CONNECT_API_KEY",
    "run-deploy-windows": "cypress run --spec ./cypress/e2e/contract/*.cy.js --env token=token,CYPRESS_CONNECT_ADDRESS=%CYPRESS_CONNECT_ADDRESS%,CONNECT_API_KEY=%CONNECT_API_KEY%"
  },
  "devDependencies": {
    "cypress": "^13.3.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "start-server-and-test": "^2.0.3"
  }
}
